<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                          xmlns:e="clr-namespace:X_Guide.Enums">
    <ObjectDataProvider x:Key="ManiType" MethodName="GetAllIntAndDescriptions" ObjectType="{x:Type e:EnumHelperClass}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="e:ManipulatorType"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
    <ObjectDataProvider x:Key="TermiType" MethodName="GetAllValuesAndDescriptions" ObjectType="{x:Type e:EnumHelperClass}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="e:Terminator"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
    <ObjectDataProvider x:Key="SoftwareType" MethodName="GetAllIntAndDescriptions" ObjectType="{x:Type e:EnumHelperClass}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="e:VisionSoftware"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
    <ObjectDataProvider x:Key="UserRole" MethodName="GetAllIntAndDescriptions" ObjectType="{x:Type e:EnumHelperClass}">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="e:UserRole"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
</ResourceDictionary>