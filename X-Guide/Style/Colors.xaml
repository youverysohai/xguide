<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PrimaryBlueColor" Color="#80B9EE"/>

    <SolidColorBrush x:Key="SecondaryBlueColor" Color="#EFF2FF"/>


    <SolidColorBrush x:Key="PrimaryBlackColor" Color="#2d2d2d"/>
    <SolidColorBrush x:Key="SecondaryBlackColor" Color="#2d2d30"/>
    <SolidColorBrush x:Key="DisconnectedColor" Color="#707070"/>
    <SolidColorBrush x:Key="ConnectedColor" Color="#6CA580"/>
    <SolidColorBrush x:Key="ErrorColor" Color="#AA4450"/>

</ResourceDictionary>